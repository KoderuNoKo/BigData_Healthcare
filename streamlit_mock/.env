# .env
# PostgreSQL Database Configuration for Healthcare Data Warehouse

# Connection settings for your existing PostgreSQL container
PG_HOST=localhost
PG_PORT=5432
PG_DATABASE=mimic_dw
PG_USER=dev
PG_PASSWORD=devpassword

# Connection Pool Settings
PG_MIN_CONNECTIONS=1
PG_MAX_CONNECTIONS=20

# Application Settings
APP_ENV=development
APP_DEBUG=true

# Cache Settings (in seconds)
CACHE_TTL_SHORT=300    # 5 minutes
CACHE_TTL_MEDIUM=1800  # 30 minutes
CACHE_TTL_LONG=3600    # 1 hour






